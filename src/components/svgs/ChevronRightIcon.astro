---
import { twMerge } from "tailwind-merge";
interface Props {
    class?: string;
}

const { class: cls } = Astro.props;
---

<svg
    xmlns="http://www.w3.org/2000/svg"
    fill="none"
    viewBox="0 0 24 24"
    stroke-width="2"
    stroke="currentColor"
    class={twMerge("w-6 h-6", cls)}
>
    <path
        stroke-linecap="round"
        stroke-linejoin="round"
        d="M8.25 4.5l7.5 7.5-7.5 7.5"></path>
</svg>
